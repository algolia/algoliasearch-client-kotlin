[
    {
        "objectID": "query_edits",
        "conditions": [
            {
                "anchoring": "is",
                "pattern": "mobile phone",
                "alternatives": true
            }
        ],
        "consequence": {
            "params": {
                "query": {
                    "edits": [
                        {
                            "type": "remove",
                            "delete": "mobile"
                        },
                        {
                            "type": "replace",
                            "delete": "phone",
                            "insert": "iphone"
                        }
                    ]
                }
            }
        }
    },
    {
        "objectID": "query_promo",
        "consequence": {
            "params": {
                "filters": "brand:OnePlus"
            }
        }
    },
    {
        "objectID": "query_promo_summer",
        "conditions": [
            {
                "context": "summer"
            }
        ],
        "consequence": {
            "params": {
                "filters": "model:One"
            }
        }
    }
]
